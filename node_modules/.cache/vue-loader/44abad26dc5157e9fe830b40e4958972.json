"\n        :sort-desc=\"sortDesc\"\n        hide-default-footer\n        disable-pagination=\"true\"\n      >\n        <template v-slot:header>\n          <v-row dense class=\"px-4 my-4\">\n            <v-col cols=\"12\" sm=\"12\" md=\"4\">\n              <v-text-field\n                v-model=\"search\"\n                clearable\n                flat\n                solo-inverted\n                hide-details\n                prepend-inner-icon=\"search\"\n                label=\"Search\"\n                dense\n              />\n            </v-col>\n            <v-col cols=\"12\" sm=\"12\" md=\"3\">\n              <v-select\n                v-model=\"sortBy\"\n                flat\n                solo-inverted\n                hide-details\n                :items=\"keys\"\n                prepend-inner-icon=\"filter_list\"\n                label=\"Sort by\"\n                dense\n              />\n            </v-col>\n            <v-col cols=\"4\" sm=\"4\" md=\"2\">\n              <v-btn-toggle v-model=\"sortDesc\" mandatory>\n                <v-btn large depressed :value=\"false\" class=\"dense-button\">\n                  <v-icon>mdi-arrow-up</v-icon>\n                </v-btn>\n                <v-btn large depressed :value=\"true\" class=\"dense-button\">\n                  <v-icon>mdi-arrow-down</v-icon>\n                </v-btn>\n              </v-btn-toggle>\n            </v-col>\n            <v-col cols=\"12\" sm=\"12\" md=\"3\" class=\"text-right\">\n              <v-btn\n                color=\"secondary\"\n                v-on:click=\"createClass()\"\n                class=\"btnNewItem pr-4\"\n              >\n                <v-icon class=\"mr-2\">mdi-plus</v-icon>\n                {{ $t('dataTable.NEW_ITEM') }}\n              </v-btn>\n            </v-col>\n          </v-row>\n        </template>\n        <template v-slot:default=\"props\">\n          <v-row no-gutters>\n            <v-col\n              v-for=\"item in props.items\"\n              :key=\"item.name\"\n              cols=\"12\"\n              sm=\"6\"\n              md=\"4\"\n            >\n              <v-card class=\"ma-4\" max-width=\"500\">\n                <v-card-title>{{ item.name }}</v-card-title>\n                <v-card-subtitle>{{\n                  `${item.mentorships.length} Mentorships`\n                }}</v-card-subtitle>\n                <v-card-text class=\"card-info-item\">\n                  {{ item.schedule }}\n                </v-card-text>\n                <v-card-text class=\"card-info-item\">\n                  {{ item.location }}\n                </v-card-text>\n                <v-card-text class=\"card-info-item\">\n                  {{ item.info }}\n                </v-card-text>\n                <!-- <div class=\"card-spacer\" /> -->\n                <v-card-actions>\n                  <v-btn\n                    color=\"blue\"\n                    class=\"action-button\"\n                    text\n                    v-on:click=\"editClass(item._id)\"\n                    >EDIT</v-btn\n                  >\n\n                  <v-btn\n                    color=\"red\"\n                    class=\"action-button\"\n                    text\n                    v-on:click=\"handleDeleteClass(item._id)\"\n                  >\n                    DELETE\n                  </v-btn>\n\n                  <v-spacer></v-spacer>\n                </v-card-actions>\n              </v-card>\n            </v-col>\n          </v-row>\n        </template>\n\n        <!-- <template v-slot:footer>\n          <v-row class=\"mt-2\" align=\"center\" justify=\"center\">\n            <span class=\"grey--text\">Items per page</span>\n            <v-menu offset-y>\n              <template v-slot:activator=\"{ on }\">\n                <v-btn dark text color=\"primary\" class=\"ml-2\" v-on=\"on\">\n                  {{ itemsPerPage }}\n                  <v-icon>mdi-chevron-down</v-icon>\n                </v-btn>\n              </template>\n              <v-list>\n                <v-list-item\n                  v-for=\"(number, index) in itemsPerPageArray\"\n                  :key=\"index\"\n                  @click=\"updateItemsPerPage(number)\"\n                >\n                  <v-list-item-title>{{ number }}</v-list-item-title>\n                </v-list-item>\n              </v-list>\n            </v-menu>\n\n            <v-spacer></v-spacer>\n\n            <span class=\"mr-4 grey--text\">\n              Page {{ page }} of {{ numberOfPages }}\n            </span>\n            <v-btn\n              fab\n              dark\n              color=\"blue darken-3\"\n              class=\"mr-1\"\n              @click=\"formerPage\"\n            >\n              <v-icon>mdi-chevron-left</v-icon>\n            </v-btn>\n            <v-btn\n              fab\n              dark\n              color=\"blue darken-3\"\n              class=\"ml-1\"\n              @click=\"nextPage\"\n            >\n              <v-icon>mdi-chevron-right</v-icon>\n            </v-btn>\n          </v-row>\n        </template> -->\n      </v-data-iterator>\n    </v-container>\n    <ErrorMessage />\n    <SuccessMessage />\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\n// import { getFormat, buildPayloadPagination } from '@/utils/utils.js'\nimport EditForm from './ClassEditForm.vue'\n\nexport default {\n  components: {\n    EditForm\n  },\n  metaInfo() {\n    return {\n      title: this.$store.getters.appTitle,\n      titleTemplate: `${this.$t('events.TITLE')} - %s`\n    }\n  },\n  data() {\n    return {\n      dialog: false,\n      id: '123',\n      currentClass: {},\n      mode: '',\n      search: '',\n      sortBy: 'name',\n      keys: ['name', 'info', 'schedule'],\n      filter: {},\n      sortDesc: false\n    }\n  },\n  computed: {\n    ...mapGetters(['getClassById']),\n    classes() {\n      return this.$store.state.classes.classes\n    },\n    filteredKeys() {\n      return this.keys.filter((key) => key !== `Name`)\n    }\n  },\n  watch: {},\n  methods: {\n    ...mapActions(['getAllClasses', 'deleteClass']),\n    editClass(id) {\n      this.dialog = true\n      this.mode = 'edit'\n      this.classId = id\n    },\n    createClass() {\n      this.dialog = true\n      this.mode = 'create'\n    },\n    async handleDeleteClass(id) {\n      const response = await this.$confirm(\n        this.$t('common.DO_YOU_REALLY_WANT_TO_DELETE_THIS_ITEM'),\n        {\n          title: this.$t('common.WARNING'),\n          buttonTrueText: this.$t('common.DELETE'),\n          buttonFalseText: this.$t('common.CANCEL'),\n          buttonTrueColor: 'red'\n          // buttonFalseColor: 'green'\n        }\n      )\n      if (response) {\n        await this.deleteClass(id)\n      }\n    },\n    close() {\n      this.dialog = false\n      this.classId = ''\n    }\n  },\n  async mounted() {\n    await this.getAllClasses()\n  }\n}\n</script>\n\n<style>\n.card-info-item {\n  padding: 0px 16px 8px 16px;\n}\n.card-spacer {\n  padding: 8px 16px 8px 16px;\n}\n\n.action-button {\n  min-width: 0px !important;\n}\n\n.dense-button {\n  height: 38px !important;\n}\n</style>\n"]}]}