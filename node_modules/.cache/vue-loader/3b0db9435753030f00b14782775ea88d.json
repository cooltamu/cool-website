           <v-flex xs12 md6>\n                <label for=\"createdAt\">{{ $t('common.CREATED') }}</label>\n                <div name=\"createdAt\">\n                  {{ getFormat(data.createdAt) }}\n                </div>\n              </v-flex>\n              <v-flex xs12 md6>\n                <label for=\"updatedAt\">{{ $t('common.UPDATED') }}</label>\n                <div name=\"updatedAt\">\n                  {{ getFormat(data.updatedAt) }}\n                </div>\n              </v-flex>\n            </template>\n            <v-form ref=\"form\" v-model=\"valid\" lazy-validation id=\"edit-form\">\n              <v-flex xs12>\n                <v-text-field\n                  v-model=\"formData.name\"\n                  :counter=\"10\"\n                  :rules=\"rules.name\"\n                  label=\"Name\"\n                  required\n                />\n              </v-flex>\n              <v-text-field\n                v-model=\"formData.schedule\"\n                :rules=\"rules.schedule\"\n                label=\"Schedule\"\n                required\n              />\n              <v-text-field\n                v-model=\"formData.location\"\n                :rules=\"rules.location\"\n                label=\"Location\"\n                required\n              />\n              <v-text-field\n                v-model=\"formData.info\"\n                :rules=\"rules.info\"\n                label=\"Info\"\n                required\n              />\n            </v-form>\n          </v-layout>\n        </v-container>\n      </v-card-text>\n\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"red\" text @click=\"close\" class=\"btnCancel\">{{\n          $t('dataTable.CANCEL')\n        }}</v-btn>\n        <v-btn\n          color=\"green\"\n          text\n          @click=\"save\"\n          class=\"btnSave\"\n          :disabled=\"!valid\"\n          >{{ $t('dataTable.SAVE') }}</v-btn\n        >\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport { mapActions } from 'vuex'\nimport _ from 'lodash'\nimport { getFormat, buildPayloadPagination } from '@/utils/utils.js'\n\nexport default {\n  name: 'ClassEditForm',\n  metaInfo() {\n    return {\n      title: this.$store.getters.appTitle,\n      titleTemplate: `${this.$t('events.TITLE')} - %s`\n    }\n  },\n  props: ['data', 'mode'],\n  data() {\n    return {\n      formData: _.cloneDeep(\n        _.pick(this.data, ['name', 'schedule', 'location', 'info', '_id'])\n      ),\n      valid: true,\n      dialog: true,\n      item: { name: '', schedule: '' },\n      rules: {\n        name: [(v) => !!v || 'Name is required'],\n        schedule: [(v) => !!v || 'Schedule is required'],\n        location: [(v) => !!v || 'Location is required'],\n        info: []\n      }\n    }\n  },\n  computed: {},\n  watch: {},\n  methods: {\n    ...mapActions(['editClass', 'createClass']),\n    validate() {\n      this.$refs.form.validate()\n    },\n    reset() {\n      this.$refs.form.reset()\n    },\n    resetValidation() {\n      this.$refs.form.resetValidation()\n    },\n    close() {\n      this.$emit('close')\n    },\n    async save() {\n      if (this.valid) {\n        if (this.mode === 'create') {\n          await this.createClass(this.formData)\n        } else if (this.mode === 'edit') {\n          await this.editClass(this.formData)\n        }\n        this.$emit('close')\n      }\n    },\n    getFormat(date) {\n      window.__localeId__ = this.$store.getters.locale\n      return getFormat(date, 'iii, MMMM d yyyy, h:mm a')\n    }\n  }\n}\n</script>\n\n<style>\n.card-info-item {\n  padding: 0px 16px 8px 16px;\n}\n.card-spacer {\n  padding: 8px 16px 8px 16px;\n}\n#edit-form {\n  width: 100%;\n}\n</style>\n"]}]}